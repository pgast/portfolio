rops} setView={setView} />\n            )}\n          />\n          <Route exact path={\"/work\"} component={Work} />\n          <Route exact path={\"/about\"} component={About} />\n        </div>\n      </Router>\n    </ThemeProvider>\n  );\n}\n\nexport default App;","/Users/pablogl/Documents/Dev/portfolio/src/constants/theme.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Navigation/index.js",["93","94","95"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\n\nimport { MobileContact } from '../Contact';\n\nimport {\n  Container,\n  DesktopNavbar,\n  NavLogo,\n  NavLinks,\n  NavLink,\n  MobileMenu,\n  MobileMenuSectionLinks,\n  MobileMenuContactLinks,\n  MobileMenuIcon,\n  ContactIcon,\n  LinkedInIcon,\n} from './styled'\n\nconst Navigation = ({ setView, view, setDrawerOpen }) => {\n  const [ mobileMenuOpen, setMobileMenuOpen ] = useState(false);\n  const setMobileView = (view) => {\n    setView(view);\n    setMobileMenuOpen(false);\n  };\n\n  // TODO Separate render for mobile menu and desktop\n\n  return (\n    <Container>\n      <DesktopNavbar>\n        <Link \n          to='/' \n          onClick={() => setView(null)}\n          style={{ textDecoration: \"none\", color: \"black\" }} \n        >\n          <NavLogo>\n            pg\n          </NavLogo>\n        </Link>\n        <NavLinks>\n          {['work', 'about'].map(el => (\n            <Link \n              key={el}\n              to={`/${el}`} \n              onClick={() => setView(el)} \n              style={{ textDecoration: \"none\" }} \n            >\n              <NavLink $isSelected={view === el}>\n                {el}\n              </NavLink>\n            </Link>\n          ))}\n          <NavLink onClick={() => setDrawerOpen(true)}>contact</NavLink>\n        </NavLinks>\n      </DesktopNavbar>\n\n      <MobileMenuIcon \n        className={`fas fa-${mobileMenuOpen ? \"times\" : \"bars\"}`}\n        onClick={() => setMobileMenuOpen(mobileMenuOpen ? false : true)}\n      />\n\n      <MobileMenu $isVisible={mobileMenuOpen}>\n        <MobileMenuSectionLinks $isVisible={view !== 'contact'}>\n          {['work', 'about'].map(el => (\n            <Link \n              key={el}\n              to={`/${el}`} \n              onClick={() => setMobileView(el)} \n              style={{ textDecoration: \"none\" }} \n            >\n              <p>{el}</p>\n            </Link>\n          ))}  \n          <p onClick={() => setView('contact')}>contact</p> \n        </MobileMenuSectionLinks>\n\n\n        <MobileContact \n          show={view === 'contact'} \n          goToMobileMenuMain={() => setView(null)}\n        />\n\n\n        {/* <MobileMenuContactLinks>\n          <a \n            target=\"_blank\" \n            rel=\"noreferrer\"\n            href=\"https://www.linkedin.com/in/pablogastelum\" \n          >\n            <LinkedInIcon className=\"fab fa-linkedin\" />\n          </a>\n          <a href=\"mailto: pablo.g.leyva@gmail.com\">\n            <ContactIcon className=\"fas fa-envelope\" />\n          </a>\n        </MobileMenuContactLinks> */}\n      </MobileMenu>\n    </Container>\n  );\n};\n\nexport default Navigation;","/Users/pablogl/Documents/Dev/portfolio/src/components/Work/index.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Contact/index.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Home/index.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/About/index.js",["96"],"import React from 'react';\nimport './index.css';\nimport {\n  about,\n  awards,\n  skills,\n  buttons,\n  skillTech,\n  education,\n  workHistory,\n} from '../../constants/about'\n\nimport {\n  Container,\n  Scroll,\n  Button\n} from './styled'\n\nconst renderBtns = (btns) => {\n  return btns.map(btn => {\n    return (\n      <Button>\n        <a \n          target=\"_blank\" \n          rel=\"noreferrer\"\n          href={btn.href}\n        >\n          <h3>{btn.text}</h3>\n        </a>\n      </Button>\n    )\n  })\n}\n\nconst About = () => {\n  return (\n    <Container>\n      <div className=\"aboutMain\">\n        <div className=\"aboutPhoto\"></div>\n        <div className=\"aboutMain_desc\">\n          <div>\n            <h3>{about.mainDescription.h3}</h3>\n            <p>{about.mainDescription.description}</p>\n            <p>{about.mainDescription.greet}</p>\n          </div>\n          <div className=\"aboutMain_btnRow\">\n            {renderBtns(buttons)}\n          </div>\n        </div>\n      </div>\n      <Scroll>\n        <div className=\"aboutSection\">\n          <h3>{skillTech.title}</h3>\n          <div className=\"skillsTech\">\n            <ul>\n              {skillTech.columns[0].map(el => <li>{el}</li>)}\n            </ul>\n            <ul>\n              {skillTech.columns[1].map(el => <li>{el}</li>)}\n            </ul>\n          </div>\n        </div>\n        <div className=\"aboutSection\">\n          <h3>\n            {skills.title}\n          </h3>          \n          <div className=\"skillsTech\">\n            <ul>\n              {skills.columns[0].map(el => <li>{el}</li>)}\n            </ul>\n            <ul>\n              {skills.columns[1].map(el => <li>{el}</li>)}\n            </ul>\n          </div>\n        </div>\n        <div id=\"awardsEducation\">\n          <div className=\"aboutSection\">\n            <h3>\n              {awards.title}\n            </h3>\n            <div>\n              <div>\n                {awards.columns[0].map(el => <p>{el}</p>)}\n              </div>\n              <div>\n                {awards.columns[1].map(el => <p>{el}</p>)}\n              </div>\n            </div>\n          </div>\n          <div className=\"aboutSection\">\n            <h3>\n              {education.title}\n            </h3>\n            <div>\n              <div>\n                {education.school.map(el => <p>{el}</p>)}\n              </div>\n              <div>\n                <p>{education.certifications.title}</p>\n                <ul>\n                  {education.certifications.links.map(el => \n                    <li>\n                      <a\n                        target={el.href === '' ? '' : \"_blank\"}\n                        rel=\"noreferrer\"\n                        href={el.href === '' ? 'javascript:void(0);' : el.href}\n                      >\n                        {el.title}\n                      </a>\n                    </li>\n                  )}\n                </ul>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div className=\"aboutSection\" id=\"experience\">\n          <h3>\n            {workHistory.title}\n          </h3>\n          <div>\n            {workHistory.jobs.map(el => \n              <div>\n                <p>\n                  {el.title}\n                </p>\n                <ul>\n                  {el.items.map(item => <li>{item}</li>)}\n                </ul>\n              </div>\n            )}\n          </div>\n        </div>\n      </Scroll>\n    </Container>\n  );\n};\n\nexport default About;","/Users/pablogl/Documents/Dev/portfolio/src/components/Cursor/index.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Work/styled.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/constants/work.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/constants/contact.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Contact/styled.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Navigation/styled.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/constants/home.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/Home/styled.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/About/styled.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/constants/about.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/constants/animations.js",[],"/Users/pablogl/Documents/Dev/portfolio/src/components/SideDrawer/styled.js",[],"/Users/pablogl/Documents/Dev